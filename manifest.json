{
    "manifest_version": 2,
    "name": "Tab Manager Pro",
    "version": "1.0",
    "description": "Manage and save your browser tabs and workspaces.",
    "permissions": [
      "tabs",
      "storage",
      "bookmarks"
    ],
    "browser_action": {
      "default_popup": "popup.html"
    },
    "background": {
      "scripts": ["background.js"]
    }
  }
  